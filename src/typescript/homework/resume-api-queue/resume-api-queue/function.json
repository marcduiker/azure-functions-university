{
  "bindings": [
    {
      "name": "myAccomplishmentMessage",
      "type": "queueTrigger",
      "direction": "in",
      "queueName": "accomplishment-items",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "myAccomplishmentBlob",
      "type": "blob",
      "direction": "out",
      "path": "accomplishments/stored-accomplishment-{id}-{date}.json"
    }
  ],
  "scriptFile": "../dist/resume-api-queue/index.js"
}